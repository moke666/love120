{% extends "./views/shard.njk" %}
{% block css %}
    <link rel="stylesheet" type="text/css" href="../static/css/services/have_an_affair.css">
{% endblock %}
{% block content %}
    <!-- 内容 -->
    <div class="qw-container qw-resolve-contradictions">
        <!-- 轮播图 -->
        <div class="qw-resolve-view">
            <img src="../static/images/affair/banner.jpg">
        </div>
        <!-- 团队优势 -->
        <div class="qw-home-advantage">
            <div class="qw-wrapper">
                <div class="qw-advantage-listbox">
                    <ul class="qw-advantage-list qw-clear">
                        <li class="qw-advantage-item qw-left">
                            <img src="../static/images/affair/c_01_01.png">
                        </li>
                        <li class="qw-advantage-item qw-left">
                            <img src="../static/images/affair/c_01_02.png">
                        </li>
                        <li class="qw-advantage-item qw-left">
                            <img src="../static/images/affair/c_01_03.png">
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- 矛盾种类 -->
        <div class="qw-home-mid-banner">
            <div class="qw-wrapper">
                <div class="qw-mid-banner-title">
                    <img src="../static/images/affair/c_02_00.png">
                </div>
                <div class="qw-mid-banner-listbox-wrapper">
                    <div class="qw-mid-banner-listbox">
                        <ul class="qw-mid-banner-list qw-clear">
                            <li class="qw-mid-banner-item qw-left"><img src="../static/images/affair/c_02_01.png"></li>
                            <li class="qw-mid-banner-item qw-left"><img src="../static/images/affair/c_02_02.png"></li>
                            <li class="qw-mid-banner-item qw-left"><img src="../static/images/affair/c_02_05.png"></li>
                            <li class="qw-mid-banner-item qw-left"><img src="../static/images/affair/c_02_04.png"></li>
                           
                        </ul>
                    </div>
                    <a class="qw-mid-banner-prev"><img src="../static/images/affair/c_02_prev.png"></a>
                    <a class="qw-mid-banner-next"><img src="../static/images/affair/c_02_next.png"></a>
                </div>
            </div>
        </div>
        <!-- 探究问题 -->
        <div class="qw-home-explore">
            <div class="qw-wrapper">
                <div class="qw-explore-title">
                    <img src="../static/images/affair/c_03_00.png">
                </div>
                <div class="qw-explore-content qw-clear">
                    <div class="qw-explore-left qw-left">
                        <img src="../static/images/affair/c_03_bg.png">
                    </div>
                    <div class="qw-explore-right qw-right">
                        <ul class="qw-explore-list">
                            <li class="qw-explore-item"><img src="../static/images/affair/c_03_01.png"></li>
                            <li class="qw-explore-item"><img src="../static/images/affair/c_03_02.png"></li>
                            <li class="qw-explore-item"><img src="../static/images/affair/c_03_03.png"></li>
                            <li class="qw-explore-item"><img src="../static/images/affair/c_03_04.png"></li>
                            <li class="qw-explore-item"><img src="../static/images/affair/c_03_05.png"></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
       
        <!-- 化解矛盾 -->
        <div class="qw-home-defuse">
            <div class="qw-wrapper">
                <div class="qw-defuse-title">
                    <img src="../static/images/affair/c_05_00.png">
                </div>
                <div class="qw-defuse-listbox">
                    <ul class="qw-defuse-list qw-clear">
                        <li class="qw-defuse-item qw-left">
                            <img class="qw-defuse-item-zoom" src="../static/images/affair/c_05_01.jpg">
                            <img class="qw-defuse-item-fix-b" src="../static/images/affair/c_05_02.png">
                        </li>
                        <li class="qw-defuse-item qw-left">
                            <div class="qw-fix-t-padding">
                                <img class="qw-defuse-item-fix-t" src="../static/images/affair/c_05_03.png">
                            </div>
                            <div class="qw-item-zoom-wrapper">
                                <img class="qw-defuse-item-zoom" src="../static/images/affair/c_05_04.jpg">
                            </div>
                        </li>
                        <li class="qw-defuse-item qw-left">
                            <img class="qw-defuse-item-zoom" src="../static/images/affair/c_05_05.jpg">
                            <img class="qw-defuse-item-fix-b" src="../static/images/affair/c_05_06.png">
                        </li>
                        <li class="qw-defuse-item qw-left">
                            <div class="qw-fix-t-padding">
                                <img class="qw-defuse-item-fix-t" src="../static/images/affair/c_05_07.png">
                            </div>
                            <div class="qw-item-zoom-wrapper">
                                <img class="qw-defuse-item-zoom" src="../static/images/affair/c_05_08.jpg">
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- 收获好评 -->
        <div class="qw-home-reputation">
            <div class="qw-wrapper">
                <div class="qw-reputation-title">
                    <img src="../static/images/affair/c_06_00.png">
                </div>
                <div class="qw-reputation-content qw-clear">
                        <img src="../static/images/affair/c_06_01.png">
                </div>
            </div>
        </div>
     
     
    <script src="./static/js/services/have_an_affair.js"></script>
   {% endblock %}